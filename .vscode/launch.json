        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "internalConsole",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development",
          "ASPNETCORE_URLS": "http://localhost:5000/"
        },
      },
    ],
    "compounds": [
      {
        "name": "Run-All-Dev",
        "configurations": [
          "dev",
        ]
      },
    ]
