        "request": "launch",
        "preLaunchTask": "watch",
        "program": "${workspaceFolder}/bin/Debug/net6.0/dotnet-signalr-starter.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "internalConsole",
